<?xml version="1.0"?>
<launch>
	<!-- <param name="robot_description" textfile="$(find panporter_pkg)/urdf/panporter.urdf.xacro" /> -->
  	<param name="robot_description" command="$(find xacro)/xacro --inorder $(find panporter_pkg)/urdf/panporter.urdf.xacro my_namespace:=panporter1"/>

  	<include file="$(find gazebo_ros)/launch/empty_world.launch">
		<arg name="world_name" value="$(find panporter_pkg)/worlds/maze.world"/>
		<!-- <arg name="world_name" value="$(find panporter_pkg)/worlds/arxiv_maze.world"/> -->
		<arg name="paused" value="false"/>
		<arg name="use_sim_time" value="true"/>
		<arg name="gui" value="true"/>
		<arg name="headless" value="false"/>
		<arg name="debug" value="false"/>
  	</include>

	<include file="$(find panporter_pkg)/launch/panporter_spawner.launch">
		<arg name="namespace" value="panporter1"/>
		<arg name="model" value="panporter1"/>
		<arg name="x_pos" value="-2.0"/>
		<arg name="y_pos" value="-12.5"/> <!-- -0.5 -->
		<arg name="z_pos" value="0.35"/>
		<arg name="gui" value="False" />
	</include>
	
</launch>
